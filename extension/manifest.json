{
    "manifest_version": 3,
    "name": "ClassGPT",
    "version": "1.0",

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "ClassGPT"
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}